<resources>
  <layout id="MainLayout">
    <label text="Target" x="20" y="center"
      font="Gfx.FONT_SMALL" color="Gfx.COLOR_WHITE"
      justification="Gfx.TEXT_JUSTIFY_LEFT" />
    <label id="target_value" x="center" y="center"
      font="Gfx.FONT_LARGE" color="Gfx.COLOR_WHITE"
      justification="Gfx.TEXT_JUSTIFY_CENTER" />

    <label text="Distance" x="130" y="22"
      font="Gfx.FONT_SMALL" color="Gfx.COLOR_WHITE"
      justification="Gfx.TEXT_JUSTIFY_LEFT" />
    <label id="distance_value" x="130" y="40"
      font="Gfx.FONT_MEDIUM" color="Gfx.COLOR_WHITE"
      justification="Gfx.TEXT_JUSTIFY_LEFT" />

    <label text="Reps" x="45" y="22"
      font="Gfx.FONT_SMALL" color="Gfx.COLOR_WHITE"
      justification="Gfx.TEXT_JUSTIFY_LEFT" />
    <label id="reps_value" x="45" y="40"
      font="Gfx.FONT_MEDIUM" color="Gfx.COLOR_WHITE"
      justification="Gfx.TEXT_JUSTIFY_LEFT" />

    <label text="Rest" x="40" y="135"
      font="Gfx.FONT_SMALL" color="Gfx.COLOR_WHITE"
      justification="Gfx.TEXT_JUSTIFY_LEFT" />
    <label id="rest_value" x="center" y="130"
      font="Gfx.FONT_LARGE" color="Gfx.COLOR_WHITE"
      justification="Gfx.TEXT_JUSTIFY_CENTER" />
  </layout>

  <menu id="WorkoutMenu">
    <menu-item id="menu_item_workout_plan">Workout Plan</menu-item>
    <menu-item id="menu_item_workout_settings">Workout Settings</menu-item>
  </menu>

  <menu id="WorkoutPlanMenu">
    <menu-item id="menu_item_workout_plan_distance">Distance</menu-item>
    <menu-item id="menu_item_workout_plan_target">Target</menu-item>
    <menu-item id="menu_item_workout_plan_reps">Reps</menu-item>
    <menu-item id="menu_item_workout_plan_rest">Rest</menu-item>
  </menu>

  <menu id="WorkoutSettingsMenu">
    <menu-item id="menu_item_workout_settings_start">Start</menu-item>
    <menu-item id="menu_item_workout_settings_stop">Stop</menu-item>
  </menu>

  <menu id="WorkoutSettingsStartMenu">
    <menu-item id="menu_item_workout_settings_start_on_button">Start on button</menu-item>
    <menu-item id="menu_item_workout_settings_start_on_countdown">Start on coutdown</menu-item>
  </menu>

  <menu id="WorkoutSettingsStopMenu">
    <menu-item id="menu_item_workout_settings_stop_on_button">Stop on button</menu-item>
    <menu-item id="menu_item_workout_settings_stop_on_target">Stop on target</menu-item>
  </menu>


  <strings>
    <string id="Device">fr630</string>
    <string id="isTouchScreen">false</string>
  </strings>
</resources>
